{
  "name": "e2d3",
  "version": "0.4.0",
  "authors": [
    "E2D3"
  ],
  "description": "E2D3 is JavaScript library for using D3.js on Excel.",
  "license": "E2D3",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "requirejs": "~2.2.0",
    "requirejs-domready": "~2.0.1",
    "requirejs-text": "~2.0.13",
    "bootstrap-sass-official": "~3.3.6",
    "d3": "~3.5.16",
    "font-awesome": "~4.5.0",
    "FileSaver": "1.3.0",
    "canvg-gabelerner": "1.5.1",
    "rgbcolor": "1.0.1",
    "stackblur-canvas": "1.4.1",
    "marked": "~0.3.5",
    "vue": "~1.0.20",
    "vue-i18n": "~2.4.1",
    "colorbrewer": "~1.0.0",
    "js-yaml": "3.5.5"
  },
  "overrides": {
    "requirejs-domready": {
      "main": "domReady.js"
    },
    "requirejs-text": {
